{
  "name": "capacitor-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build-ios": "vite build && npx cap sync ios && npx cap copy ios && npx cap open ios",
    "build-android": "vite build && npx cap sync android && npx cap copy android && npx cap open android",
    "preview": "vite preview",
    "upgrade": "npm install --upgrade @capacitor/core @capacitor/android @capacitor/ios vite"
  },
  "dependencies": {
    "@capacitor-trancee/bridgefy": "file:..",
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "vite": "^7.1.6"
  },
